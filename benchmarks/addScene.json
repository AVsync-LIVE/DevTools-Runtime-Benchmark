{
  "benchmarkName": "AddScene",
  "benchmarkDescription": "Add a Scene by clicking the Add Scene tile",
  "url": "http://localhost:3000/deck",
  "commands": [
    {
      "command": "wait",
      "payload": "12000",
      "description": "Wait for the startup scripts to finish executing"
    },
    {
      "command": "trace",
      "payload": "start",
      "description": "Start tracing"
    },
    {
      "command": "wait",
      "payload": "500",
      "description": "Wait"
    },
    {
      "command": "click",
      "payload": "#addScene",
      "description": "Click the Add Scene tile in the deck"
    },
    {
      "command": "wait",
      "payload": "2000",
      "description": "Wait for the Scenes in the modal to load"
    },
    {
      "command": "click",
      "payload": "#addScene_0",
      "description": "Click the first Scene in the list to add it to the Playlist"
    },
    {
      "command": "wait",
      "payload": "1000",
      "description": "Wait"
    },
    {
      "command": "click",
      "payload": "#play",
      "description": "Click the Play button to play the Playlist"
    },
    {
      "command": "wait",
      "payload": "6000",
      "description": "Wait"
    },
    {
      "command": "trace",
      "payload": "stop",
      "description": "Stop tracing"
    }
  ]
}